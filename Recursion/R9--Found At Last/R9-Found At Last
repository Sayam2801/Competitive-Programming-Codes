package problems;

import java.util.Scanner;

public class R9 {
    public static int lastindex(int N, int a[],int M,int index) {
    	if(index==N)
    		return -1;
    	int li=lastindex(N,a,M,index+1);
    	if(li!=-1)
    		return li;
    	if(M==a[index])
    		return index;
    	return li;
    }
	public static void main(String[] args) {
		// TODO Auto-generated method stub
        Scanner sc=new Scanner(System.in);
        int N=sc.nextInt();
        int a[]=new int[N];
        for(int i=0;i<N;i++)
        	a[i]=sc.nextInt();
        int M=sc.nextInt();
        int res=lastindex(N,a,M,0);
        System.out.println(res);
        sc.close();
	}

}
