package problems;

import java.util.Scanner;

public class AllIndexes {
    public static void allindex(int a[],int N,int M,int index) {
    	if(index==N)
    		return;
    	if(M==a[index])
    		System.out.print(index+" ");
    	allindex(a,N,M,index+1);
    }
	public static void main(String[] args) {
		// TODO Auto-generated method stub
        Scanner sc=new Scanner(System.in);
        int N=sc.nextInt();
        int M=sc.nextInt();
        int a[]=new int[100];
        for(int i=0;i<N;i++)
        	a[i]=sc.nextInt();
        allindex(a,N,M,0);
        sc.close();
	}

}
